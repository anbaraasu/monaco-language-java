{
  "name": "example-react-client",
  "private": true,
  "type": "module",
  "dependencies": {
    "monaco-editor-workers": "0.34.2",
    "monaco-languageclient": "../../client",
    "vscode-ws-jsonrpc": "../../vscode-ws-jsonrpc",
    "normalize-url": "~8.0.0",
    "react": "~18.2.0",
    "@monaco-editor/react": "^4.3.1",
    "react-dom": "~18.2.0"
  },
  "devDependencies": {
    "@types/react": "~18.0.25",
    "@types/react-dom": "~18.0.9"
  },
  "volta": {
    "node": "16.18.1",
    "npm": "8.19.3"
  },
  "scripts": {
    "clean": "npx shx rm -fr dist tsconfig.tsbuildinfo",
    "lint": "eslint src --ext .tsx",
    "compile": "tsc",
    "watch": "tsc -w",
    "copy:monacoworkers": "shx mkdir -p dist && shx cp -r ../../../node_modules/monaco-editor-workers/dist/workers/editorWorker* ./dist",
    "build:msg": "echo Building react-client example:",
    "build": "npm run build:msg && npm run clean && npm run compile && npm run webpack:prepare && npx webpack && npm run copy:monacoworkers"
  }
}